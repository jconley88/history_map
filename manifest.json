{
  "name": "History Map",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Organizes the history by domain",
  "chrome_url_overrides" : {
    "history": "src/history.html"
  },
  "background": {
    "scripts": ["vendor/prototype.js", "vendor/async.js", "src/log_tab_urls.js", "src/history_list.js"]
  },
  "permissions": [
    "history", "chrome://favicon/", "tabs", "webNavigation", "storage"
  ]
}