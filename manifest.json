{
  "name": "History Map",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Organizes the history by domain",
  "chrome_url_overrides" : {
    "history": "history.html"
  },
  "background": {
    "scripts": ["log_tab_urls.js", "prototype.js"]
  },
  "permissions": [
    "history", "chrome://favicon/", "tabs", "webNavigation"
  ]
}